{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtKey": "mysupersecret_secretsecretsecretkey!123",
  "ReverseProxy": {
    "Routes": {
      "grpc-route": {
        "ClusterId": "currency-service",
        "Match": {
          "Path": "/user_currencies.UserCurrenciesService/{**}"
        }
      }
    },
    "Clusters": {
      "currency-service": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002"
          }
        }
      }
    }
  }
}
